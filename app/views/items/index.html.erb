<h1> Items </h1>

<table>
  <tr>
    <th> Item id </th>
    <th> Name </th>
    <th> </th>
  </tr>

  <% @items.each do |i| %>

  <tr>
    <td> <%= i.id %> </td>
    <td> <%= link_to i.name, item_path(i) %> </td>
    <td> <%= link_to "Delete", i, method: :delete, data: { confirm: "Delete?" } %> </td>
  </tr>

  <% end %>

</table>